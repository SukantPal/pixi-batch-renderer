{
  "name": "pixi-batch-renderer",
  "version": "2.2.5",
  "description": "Batch rendering library for WebGL/PixiJS applications",
  "main": "lib/pixi-batch-renderer.cjs",
  "module": "lib/pixi-batch-renderer.mjs",
  "bundle": "dist/pixi-batch-renderer.js",
  "scripts": {
    "build": "npm run compile; npm run docs",
    "docs": "jsdoc -c ./.jsdoc.conf.json --verbose -r README.md",
    "predeploy": "npm run docs",
    "deploy": "gh-pages -d docs",
    "compile": "rollup -c",
    "compile:types": "rm -rf compile && tsc; api-extractor run; echo Done"
  },
  "author": "Shukant Pal <shukantpal@outlook.com>",
  "license": "MIT",
  "keywords": [
    "pixi",
    "pixijs",
    "webgl",
    "rendering",
    "canvas",
    "batching"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SukantPal/pixi-batch-renderer"
  },
  "peerDependencies": {
    "pixi.js": "^5.1.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.7.13",
    "@pixi/eslint-config": "^1.0.1",
    "@pixi/jsdoc-template": "^2.6.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-jsdoc": "^15.12.2",
    "floss": "^3.0.0",
    "gh-pages": "^2.2.0",
    "jsdoc": "^3.6.4",
    "pixi.js": "^5.2.1",
    "rollup": "^2.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript": "^1.0.1",
    "typescript": "^3.8.3"
  }
}