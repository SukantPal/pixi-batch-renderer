    "@pixi/eslint-config": "^1.0.1",
    "@pixi/jsdoc-template": "^2.6.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jsdoc": "^15.12.2",
    "jsdoc": "^3.6.4",
    "pixi.js": "^5.2.1",
    "rollup": "^2.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-sourcemaps": "^0.5.0",
