{
  "name": "pixi-batch-renderer",
  "version": "1.0.1",
  "description": "Batch rendering library for WebGL/PixiJS applications",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile; npm run build-docs",
    "docs": "jsdoc -c ./.jsdoc.conf.json --verbose",
    "predeploy": "npm run docs",
    "deploy": "gh-pages -d docs",
    "compile": "babel src --out-dir lib --source-maps"
  },
  "author": "Shukant Pal",
  "license": "MIT",
  "keywords": [
    "pixi",
    "pixijs",
    "webgl",
    "rendering",
    "canvas",
    "batching"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SukantPal/pixi-batch-renderer"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.5.5",
    "pixi.js": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@pixi/jsdoc-template": "^2.4.2",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-plugin-jsdoc": "^15.8.0",
    "gh-pages": "^2.1.1",
    "jsdoc": "^3.6.3",
    "pixi.js": "^5.1.1"
  }
}
