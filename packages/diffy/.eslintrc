{
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "mocha": true
    },
    "globals": {
        "global": false
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "extends": [
        "@pixi/eslint-config"
    ],
    "rules": {
        "@typescript-eslint/triple-slash-reference": [
            "off"
        ],
        "spaced-comment": [
            "off"
        ],
        "no-duplicate-imports": [
            "off"
        ]
    }
}